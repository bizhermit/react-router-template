{
  "name": "web-app-template",
  "version": "0.0.0",
  "description": "web app template",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run generate-i18n-types && npx prisma generate && npx react-router build",
    "dev": "npm run generate-i18n-types && npx react-router dev --host",
    "start": "npx react-router-serve ./build/server/index.js",
    "migrate": "npx prisma migrate dev",
    "prisma": "npx prisma studio",
    "typecheck": "npx react-router typegen && tsc && npx eslint",
    "generate-i18n-types": "node --loader ts-node/esm ./scripts/generate-i18n-types.ts",
    "format": "npx eslint --fix"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "^6.11.1",
    "@react-router/node": "^7.6.0",
    "@react-router/serve": "^7.6.0",
    "isbot": "^5.1.17",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.0",
    "serialize-javascript": "^6.0.2",
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@react-router/dev": "^7.6.0",
    "@stylistic/eslint-plugin": "^5.1.0",
    "@tailwindcss/vite": "^4.1.6",
    "@types/node": "^22.15.18",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@types/serialize-javascript": "^5.0.4",
    "eslint": "^9.30.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prisma": "^6.11.1",
    "react-router-devtools": "^5.0.5",
    "sass-embedded": "^1.87.0",
    "tailwindcss": "^4.1.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.35.1",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
