{
  "name": "web-app-template",
  "version": "0.0.0",
  "description": "web app template",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "npm run generate-i18n-types & npm run generate-openapi-types && npx prisma generate",
    "build": "npx react-router build",
    "dev": "npx react-router dev --host",
    "start": "npx react-router-serve ./build/server/index.js",
    "migrate": "npx prisma migrate dev",
    "prisma": "npx prisma studio",
    "typecheck": "npx tsc && npx eslint",
    "typecheck:rebuild": "npm run prebuild && npx react-router typegen && npm run typecheck",
    "generate-i18n-types": "node ./scripts/generate-i18n-types.mjs",
    "generate-openapi-types": "npm run parse-openapi-docs && bash ./scripts/generate-openapi-types.sh",
    "parse-openapi-docs": "node ./scripts/parse-openapi-docs.mjs",
    "format": "npx eslint --fix",
    "playwright": "npx playwright test",
    "test": "npx vitest run app"
  },
  "dependencies": {
    "@auth/core": "^0.41.1",
    "@auth/prisma-adapter": "^2.11.1",
    "@prisma/client": "^7.2.0",
    "@react-router/node": "^7.11.0",
    "@react-router/serve": "^7.11.0",
    "isbot": "^5.1.32",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router": "^7.11.0",
    "serialize-javascript": "^7.0.2",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@react-router/dev": "^7.11.0",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/serialize-javascript": "^5.0.4",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "js-yaml": "^4.1.1",
    "openapi-typescript": "^7.10.1",
    "prisma": "^5.22.0",
    "react-router-devtools": "^6.0.0",
    "sass-embedded": "^1.97.1",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vite": "^7.3.0",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
